model User{
    id String @id @default(uuid())

    email String @unique
    password String

    firstName String?
    lastName String?

    role Role @default(USER)

    createdAT DateTime @default(now()) @map("created_at")
    updatedAT DateTime @updatedAt @map("updated_at")
}

enum Role {
    USER
    ADMIN
    MODERATOR
}